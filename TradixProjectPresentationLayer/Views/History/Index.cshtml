<script>
    // Dekoratör işlevi: Belirli hücrelere "highlight" sınıfı ekler
    function decorateCells(selector) {
        const cells = document.querySelectorAll(selector);
        cells.forEach(cell => {
            cell.classList.add("highlight");
        });
    }

    // Kullanım: En yüksek değer ve en düşük değer hücrelerini vurgula
    decorateCells("td:nth-child(6)"); // En Yüksek Değer sütunu
    decorateCells("td:nth-child(7)"); // En Düşük Değer sütunu
</script>
